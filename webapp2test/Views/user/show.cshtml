    <!-- sign in -->
    <div id="signInModal" class="modal fade" style="background-color: transparent;">
        <div class="modal-dialog modal-dialog-centered h-100" style="max-width: 846px; height: 500px; background-color: transparent;">
            <div class="modal-content h-75" style="background-color: transparent;">
                <div class="modal-body" style=" background-color: #b69363; border: 1px solid black; border-radius: 20px;">
                    <h3>Sign In</h3>
                    <hr id="hr1">
                    <br>
                    <div class="form-login text-center">
                     
                        <form onsubmit="return login()" method="post" asp-controller="user" asp-action="login">
                            <div id="f1">
                                <div class="form-floating mb-5">
                                    <input type="email" class="form-control" id="username" placeholder="name@example.com" asp-for="Login.Username">
                                    <span asp-validation-for="Login.Username" class="text-danger"></span>
                                    <label for="floatingInput" asp-for="Login.Username">Username</label>
                                </div>
                                <div class="form-floating">
                                    <input type="password" class="form-control" id="password" placeholder="Password" asp-for="Login.password">
                                    <span asp-validation-for="Login.password" class="text-danger"></span>
                                    <label for="floatingPassword" asp-for="Login.password">Password</label>
                                </div>
                            </div>
                            <br>
                            <button type="submit" class="btn btn-primary w-50 mt-4">LOG IN</button>
                            <br>
                            <button onclick="$('#signInModal').modal('hide'); $('#signUpModal').modal('show');" type="button" class="btn btn-primary w-50 mt-4">SIGN UP</button>
                        </form>
                    </div>
                    <div class="text-center">
                        <div id="rec20"></div>
                        <div id="rec21"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

